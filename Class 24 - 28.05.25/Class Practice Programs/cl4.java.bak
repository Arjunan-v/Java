import java.util.Scanner;
class cl4
{
	static Scanner sc= new Scanner(System.in);
	public static void main(String[] args) 
	{
		System.out.print("Enter starting range:");
		int s = sc.nextInt();
		System.out.print("Enter ending range:");
		int e = sc.nextInt();
		for (int i=s;i<=e ;i++ )
		{
			if (i%2 == 0)
			{
				System.out.println(i);
			}
			
		}
	}
}
