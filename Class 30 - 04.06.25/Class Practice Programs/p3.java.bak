class p3
{
	public static void main(String[] args) throws Exception
	{
		int n=5,a=5;
		for (int i=0;i<n ;i++ )
		{
			//a=5;
			for (int j=0,a=5;j<n ;j++ )
			{
				if (i+j<=n-1)
				{
					System.out.print((a--) +" ");
				}
				else
				{
					System.out.print("  ");
				}
				
				//Thread.sleep(1000);
			}
			System.out.println();
		}
		
	}
}

